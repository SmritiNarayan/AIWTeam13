{
  "manifest_version": 2,

  "name": "GTmetrix Analyzer Plugin",
  "description": "This extension will analyze a page using GTmetrix",
  "version": "1.0",

  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
  },
  "commands":{
          "toggle-feature-foo": {
            "suggested_key": {
              "default": "Alt+S"
            },
            "description": "Toggle feature foo"//,
           // "global": true
          }
   },
  "permissions": [
   "activeTab",
   "history",
   "topSites",
   "bookmarks",
   "tabs",
   "signedInDevices",
   "cookies",
   "downloads",
   "fontSettings",
   "privacy"
   ],
   
  "background":{
    "scripts": ["eventPage.js"],
    "persistent":false
  }
}